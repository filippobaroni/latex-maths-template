\RequirePackage{tikz}
% Maths libraries
\usetikzlibrary{calc,math}
% Node shapes library
\usetikzlibrary{shapes.geometric}

% \FilBarBulletPointI
\NewDocumentCommand{\FilBarBulletPointI}{}{%
\begin{tikzpicture}[baseline=(bullet.base)]
\node[opacity=0,inner sep=0pt,outer sep=0pt] (bullet) {$\bullet$};
\begin{pgfinterruptboundingbox}
\coordinate (bottom) at ($(bullet.north)!.875!(bullet.south)$);
\coordinate (centre) at ($(bullet.north)!.5!(bottom)$);
\tikzmath{
coordinate \a,\b;
\a=(bullet.north);
\b=(bottom);
\r=(\ay-\by)/2*0.95;
}
\node[regular polygon,regular polygon sides=6,inner sep={\r*.707*.85},draw,line width={\r*.4},rounded corners={\r*.25},fill=filbar theme color!50] at (centre) {};
\end{pgfinterruptboundingbox}
\end{tikzpicture}%
}

% \FilBarQED
\NewDocumentCommand{\FilBarQED}{}{%
\begin{tikzpicture}[baseline=(openbox.base)]
    \node[opacity=0,inner sep=0pt,outer sep=0pt,inner xsep=-1em] (openbox) {$\openbox$};
    \begin{pgfinterruptboundingbox}
    \tikzmath{
    coordinate \a,\b,\c;
    \a=(openbox.north);
    \b=(openbox.south);
    \r=(\ay-\by)/2-.2pt;
    }
    \draw[draw=.,line width=.5pt,rounded corners=2pt,shift={(openbox.center)}] (\r pt,\r pt) rectangle (-\r pt,-\r pt);
    \end{pgfinterruptboundingbox}
    \end{tikzpicture}
}